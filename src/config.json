{
    "mode": "csv",
    "sensors": [
        {
            "id": "KitchenAirQ",
            "publish_interval": 1000,
            "fields": [
                {
                    "name": "temperature",
                    "type": "DoubleField",
                    "generator": "SimpleRangeDoubleGenerator",
                    "range_from": "15.0",
                    "range_to": "23.0"
                },
                {
                    "name": "co2",
                    "type": "IntegerField",
                    "generator": "SimpleRangeIntegerGenerator",
                    "range_from": "400",
                    "range_to": "5000"
                },
                {
                    "name": "humidity",
                    "type": "DoubleField",
                    "generator": "SimpleRangeDoubleGenerator",
                    "range_from": "0",
                    "range_to": "100"
                },
                {
                    "name": "luminance",
                    "type": "IntegerField",
                    "generator": "SimpleRangeIntegerGenerator",
                    "range_from": "0",
                    "range_to": "850"
                }
            ]
        }
    ]
}