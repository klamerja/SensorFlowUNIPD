*{
    color: rgb(68, 68, 68);
}

#searchEdit{
    background-color: rgba(0,0,0,0%);
}

QComboBox#addPanel:down-arrow{
    background-color: rgba(0, 0, 0, 0);
    image: url(:/assets/QComboBox/Down.svg);
    width: 18px;
}

QWidget#card{
    border-radius: 20px;
}

QFrame#card:hover{
    border: 2px solid #9CF3FF;
    border-radius: 20px;
}

QFrame#card:focus{
    border: 1px solid #00E0FF;
    border-radius: 20px;
}

